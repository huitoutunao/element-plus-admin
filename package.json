{
  "name": "app-vue3-vite3",
  "private": true,
  "version": "0.1.0",
  "description": "app vue3 start",
  "scripts": {
    "dev": "vite --host",
    "lint:js": "eslint --cache \"./src/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:css": "stylelint --cache --cache-location node_modules/.cache/stylelint/ \"./src/**/*.{vue,scss,sass,less,styl}\" --fix",
    "format": "prettier --write .",
    "git:commit": "czg",
    "test:unit": "jest",
    "build:production": "vue-tsc --noEmit && vite build --mode production",
    "build:uat": "vue-tsc --noEmit && vite build --mode uat",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^1.2.0",
    "element-plus": "^2.2.27",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@iconify-json/ep": "^1.1.8",
    "@types/jest": "^29.2.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^29.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "cz-git": "^1.4.0",
    "czg": "^1.4.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.3",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^1.0.11",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./src/**/*.{vue,js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "./src/**/*.{vue,scss,less,styl}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
